<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="erp.parkingcard" namespace="/parkingCard" extends="erp-default">
		<action name="parkingcard" class="erp.parkingCardAction">
			<result name="toEdit">
				/WEB-INF/jsp/parkingCard/edit.jsp
			</result>
			<result name="toCreate">
				/WEB-INF/jsp/parkingCard/create.jsp
			</result>
			<result name="enable">
				/WEB-INF/jsp/parkingCard/enable.jsp
			</result>
			<result name="disable">
				/WEB-INF/jsp/parkingCard/disable.jsp
			</result>
		</action>
	</package>
</struts>
