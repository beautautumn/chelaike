<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="erp.marketAction" namespace="/market" extends="erp-default">
		<action name="market" class="erp.marketAction">
			<result name="create">
				/WEB-INF/jsp/market/create.jsp
			</result>
			<result name="edit">
				/WEB-INF/jsp/market/edit.jsp
			</result>
            <result name="frozen">
                /WEB-INF/jsp/market/frozen.jsp
            </result>
            <result name="unfrozen">
                /WEB-INF/jsp/market/unfrozen.jsp
            </result>
            <result name="cancel">
                /WEB-INF/jsp/market/cancel.jsp
            </result>
		</action>
	</package>

</struts>
